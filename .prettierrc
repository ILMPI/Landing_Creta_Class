{
	"trailingComma": "all",
	"tabWidth": 4,
	"useTabs": true,
	"semi": true,
	"singleQuote": true,
	"endOfLine": "auto",
	"bracketSpacing": true,
	"printWidth": 120,
	"arrowParens": "avoid",
	"htmlWhitespaceSensitivity": "css",
	"bracketSameLine": false,
	"singleAttributePerLine": false,
	"overrides": [
		{
			"files": "*.html",
			"options": {
				"printWidth": 1000,
				"parser": "html"
			}
		}
	]
}
